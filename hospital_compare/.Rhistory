montecarlo <- function(n) {
x <- runif(n, min = -1, max = 1)
y <- runif(n, min = -1, max = 1)
plcmt <- c()
for (i in 1:n) {
if (x^2 + y^2 >1) {
plcmt[i] <- "out"
}
else {
plcmt[i] <- "in"
}
}
return(4*(sum(plcmt=="in")/n))
}
